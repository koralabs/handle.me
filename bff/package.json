  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35"
  },
  "scripts": {
    "build": "tsc -p . && cp package.json dist/package.json && cd dist && yarn install --production && zip -9 -r release.zip .",
